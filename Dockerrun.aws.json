{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "vasianurag/multicontainer-docker-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "vasianurag/multicontainer-docker-server",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "vasianurag/multicontainer-docker-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort":80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }
    ]
}